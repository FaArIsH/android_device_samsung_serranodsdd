Signed-off-by: k2wl <k2wl@github.com>
---
 rootdir/init.qcom.rc | 1 +
 serrano-common.mk    | 4 ----
 2 files changed, 1 insertion(+), 4 deletions(-)

diff --git a/rootdir/init.qcom.rc b/rootdir/init.qcom.rc
index 7cf700d..a01000d 100644
--- a/rootdir/init.qcom.rc
+++ b/rootdir/init.qcom.rc
@@ -26,6 +26,7 @@
 #
 
 import init.qcom.usb.rc
+import init.serranods.rc
 
 on init
     # Set permissions for persist partition
diff --git a/serrano-common.mk b/serrano-common.mk
index 494f539..446ee46 100644
--- a/serrano-common.mk
+++ b/serrano-common.mk
@@ -174,10 +174,6 @@ PRODUCT_PROPERTY_OVERRIDES += \
     audio.gapless.playback.disable=true \
     qcom.hw.aac.encoder=true
 
-# RIL
-PRODUCT_PROPERTY_OVERRIDES += \
-    ro.telephony.ril_class=SamsungQualcommRIL
-
 # Enable Samsung EMS dial path
 PRODUCT_PROPERTY_OVERRIDES += \
     ro.telephony.ril.v3=samsungEMSReq
